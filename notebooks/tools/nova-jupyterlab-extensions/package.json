{
  "name": "nova",
  "version": "0.3.0",
  "description": "Jupyter Notebook submission to GCP",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/gclouduniverse/nova-jupyterlab-plugin",
  "bugs": {
    "url": "https://github.com/gclouduniverse/nova-jupyterlab-plugin/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Abdalla Abdulmonem, Viacheslav Kovalevskyi",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/*.css",
    "style/images/*.png"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/gclouduniverse/nova-jupyterlab-plugin.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.7",
    "@jupyterlab/apputils": "^1.0.0",
    "@jupyterlab/coreutils": "^3.0.0",
    "@jupyterlab/mainmenu": "^1.0.0",
    "@jupyterlab/notebook": "^3.0.7",
    "file-system": "^2.2.2",
    "fs-extra": "^7.0.1",
    "typestyle": "^2.1.0"
  },
  "devDependencies": {
  },
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyterlab_nova"
        }
      }
    }
  }
}
